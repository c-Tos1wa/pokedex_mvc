
<body>
  <main>
    <%- include('header'); %>
      <section class="section">
        <div class="card">
          <div class="eachCard">
            <img class="card-item" src="<%= pokemonById.image %>" alt=<%=pokemonById.name %> />
            <p class="card-item">
              <%= pokemonById.number %>
            </p>
            <p class="card-item">
              <%= pokemonById.name %>
            </p>
            <p class="card-item">Descrição: <%= pokemonById.description %>
            </p>
            <p class="card-item">Altura: <%= pokemonById.height %>
            </p>
            <p class="card-item">Peso: <%= pokemonById.weight %>
            </p>
            <p class="card-item">Categoria: <%= pokemonById.category %>
            </p>
            <p class="card-item">Habilidades: <%= pokemonById.abilities %>
            </p>
          </div>
        </div>
      </section>
  </main>
  <%- include('footer'); %>
    <script src='js/script.js'></script>
</body>

</html>